{
    "name": "Fanuc",
    "patterns": [
        {
            "include": "#comment"
        },
        {
            "include": "#string"
        },
        {
            "include": "#fcn-call"
        },
        {
            "include": "#numbers"
        },
        {
            "include": "#control"
        },
        {
            "include": "#control1"
        },
        {
            "include": "#control2"
        },
        {
            "include": "#control3"
        },
        {
            "include": "#control4"
        },
        {
            "include": "#control5"
        },
        {
            "include": "#control6"
        },
        {
            "include": "#control7"
        },
        {
            "include": "#movement"
        },
        {
            "include": "#movement1"
        },
        {
            "include": "#built-in-var"
        },
        {
            "include": "#built-in-fcn"
        },
        {
            "include": "#built-in-fcn1"
        },
        {
            "include": "#built-in-fcn2"
        },
        {
            "include": "#built-in-fcn3"
        },
        {
            "include": "#built-in-types"
        },
        {
            "include": "#built-in-types1"
        },
        {
            "include": "#built-in-types2"
        },
        {
            "include": "#built-in-types3"
        },
        {
            "include": "#operator"
        }
    ],
    "repository": {
        "built-in-fcn": {
            "patterns": [
                {
                    "match": "(?i)\\b(GP1|MESSAGE|UFRAME|UTOOL|PAUSE|UFRAME_NUM|UTOOL_NUM)\\b",
                    "name": "entity.name.function.c"
                }
            ]
        },
        "built-in-fcn1": {
            "patterns": [
                {
                    "match": "\\sMESSAGE\\[[a-zA-Z0-9\\s]+\\]",
                    "name": "entity.name.function.c"
                }
            ]
        },
        "built-in-fcn2": {
            "patterns": [
                {
                    "match": "\\sUTOOL\\[\\d\\]",
                    "name": "entity.name.function.c"
                }
            ]
        },
        "built-in-fcn3": {
            "patterns": [
                {
                    "match": "\\sUFRAME\\[\\d+\\]3",
                    "name": "entity.name.function.c"
                }
            ]
        },
        "built-in-types": {
            "patterns": [
                {
                    "match": "(?i)\\b(DATE|TIME|deg|mm|FINE|UF|UT|J1|J2|J3|J4|J5|J6|J7|J8|X|Y|Z|W|R|READ_WRITE|ON|OFF)\\b",
                    "name": "storage.type.c"
                }
            ]
        },
        "built-in-types1": {
            "patterns": [
                {
                    "match": "\\b[LJ][\\s]P\\[\\d+\\]",
                    "name": "storage.type.c"
                }
            ]
        },
        "built-in-types2": {
            "patterns": [
                {
                    "match": "\\sPR\\[\\d,\\d\\]",
                    "name": "storage.type.c"
                }
            ]
        },
        "built-in-types3": {
            "patterns": [
                {
                    "match": "\\PR?\\[\\d+\\]",
                    "name": "storage.type.c"
                }
            ]
        },
        "built-in-var": {
            "patterns": [
                {
                    "match": "(?i)\\b(STACK_SIZE|TASK_PRIORITY|TIME_SLICE|BUSY_LAMP_OFF|ABORT_REQUEST|PAUSE_REQUEST|CONFIG)\\b",
                    "name": "constant.language"
                }
            ]
        },
        "comment": {
            "patterns": [
                {
                    "begin": "(?<!IF.*)!",
                    "end": "(?=\\n)",
                    "name": "comment"
                }
            ]
        },
        "control": {
            "patterns": [
                {
                    "match": "(?i)\\b(PROG_SIZE|CREATE|MODIFIED|FILE_NAME|VERSION|LINE_COUNT|MEMORY_SIZE|PROTECT|TCD|DEFAULT_GROUP|CONTROL_CODE|OWNER|COMMENT)\\b",
                    "name": "keyword.control"
                }
            ]
        },
        "control1": {
            "patterns": [
                {
                    "match": "\\s\\d+:",
                    "name": "keyword.control"
                }
            ]
        },
        "control2": {
            "patterns": [
                {
                    "match": "\\/PROG",
                    "name": "keyword.control"
                }
            ]
        },
        "control3": {
            "patterns": [
                {
                    "match": "\\/ATTR",
                    "name": "keyword.control"
                }
            ]
        },
        "control4": {
            "patterns": [
                {
                    "match": "\\/MN",
                    "name": "keyword.control"
                }
            ]
        },
        "control5": {
            "patterns": [
                {
                    "match": "\\/POS",
                    "name": "keyword.control"
                }
            ]
        },
        "control6": {
            "patterns": [
                {
                    "match": "\\/END",
                    "name": "keyword.control"
                }
            ]
        },
        "control7": {
            "patterns": [
                {
                    "match": "\\d+mm/sec",
                    "name": "keyword.control"
                }
            ]
        },
        "fcn-call": {
            "patterns": [
                {
                    "begin": "CALL",
                    "end": ";",
                    "name": "string"
                }
            ]
        },
        "movement": {
            "patterns": [
                {
                    "match": "(?i)\\b(MN|POS|WAIT|DI|END)\\b",
                    "name": "entity.name.function.preprocessor.c"
                }
            ]
        },
        "movement1": {
            "patterns": [
                {
                    "match": "\\sD[OI]\\[\\d\\]",
                    "name": "entity.name.function.preprocessor.c"
                }
            ]
        },
        "numbers": {
            "patterns": [
                {
                    "match": "\\-?\\d+\\.\\d+",
                    "name": "constant.numeric.c"
                }
            ]
        },
        "operator": {
            "patterns": [
                {
                    "match": "(?i)\\b([|])\\b",
                    "name": "keyword.operator"
                }
            ]
        },
        "string": {
            "patterns": [
                {
                    "begin": "\"",
                    "end": "\"",
                    "name": "string"
                }
            ]
        }
    },
    "scopeName": "robodk.fanuc"
}