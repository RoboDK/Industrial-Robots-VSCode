{
  "name": "Industrial-Robots",
  "displayName": "Industrial Robots",
  "description": "Robot Programming Syntax Highlighting for Industrial Robots. Supported languages include: ABB RAPID, Fanuc LS, KUKA KRC, Kawasaki PRG, Yaskawa/Motoman INFORM, St√§ubli VAL3, Universal Robots Script, ...",
  "icon": "icon.png",
  "version": "0.1.4",
  "publisher": "RoboDK",
  "engines": {
    "vscode": "^1.5.0"
  },
